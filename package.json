{
    "name": "typhinet",
    "version": "1.0.0",
    "description": "typhinet server",
    "main": "server.js",
    "type": "module",
    "scripts": {
        "start": "if-env NODE_ENV=production && yarn start:prod || yarn start:dev",
        "start:prod": "node server.js",
        "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"yarn client\"",
        "client": "cd client && yarn start",
        "install": "cd client && yarn",
        "build": "cd client && yarn build",
        "predeploy": "npm run build",
        "heroku-postbuild": "yarn build"
    },
        "homepage": "https://lcerdeira.github.io/TyphiNET",
    "eslintConfig": {
        "extends": "react-app"
    },
    "dependencies": {
        "chokidar": "^3.5.1",
        "cookie-parser": "~1.4.4",
        "csv-parser": "^2.3.3",
        "debug": "~2.6.9",
        "del": "^6.0.0",
        "dom-to-image": "^2.6.0",
        "dotenv": "^8.2.0",
        "express": "~4.16.1",
        "express-async-handler": "^1.1.4",
        "fast-csv": "^4.3.6",
        "http-errors": "~1.6.3",
        "if-env": "^1.0.4",
        "material-dashboard": "^2.1.0",
        "mongodb": "^3.6.3",
        "mongoose": "^5.11.15",
        "mongoose-seed": "^0.6.0",
        "mongoose-seed-csv": "^0.0.4",
        "morgan": "~1.9.1",
        "nodemailer": "^6.4.16",
        "objects-to-csv": "^1.3.6",
        "react-scripts": "^4.0.3"
    },
    "devDependencies": {
        "browser-sync": "^2.26.13",
        "concurrently": "^5.3.0",
        "eslint": "^7.14.0",
        "mocha": "^8.2.1",
        "nodemon": "^2.0.6",
        "supertest": "^6.0.0"
    },
    "engines": {
        "node": "14.15.5"
    }
}