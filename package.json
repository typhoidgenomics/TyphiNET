        "dotenv": "^8.2.0",
        "express": "~4.16.1",
        "express-async-handler": "^1.1.4",
        "fast-csv": "^4.3.6",
        "http-errors": "~1.6.3",
        "if-env": "^1.0.4",
        "material-dashboard": "^2.1.0",
        "mongodb": "^3.6.3",
        "mongoose": "^5.11.15",
        "mongoose-seed": "^0.6.0",
        "mongoose-seed-csv": "^0.0.4",
        "morgan": "~1.9.1",
        "nodemailer": "^6.4.16",
        "objects-to-csv": "^1.3.6",
        "@fortawesome/fontawesome-svg-core": "^1.2.32",
